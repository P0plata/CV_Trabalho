[sub_resource type="Shader" id="Shader_tub6s"]
code = "shader_type spatial;
render_mode unshaded;

uniform sampler2D tex;
uniform int zona_correta = 0; // 0,1,2
uniform int modo = 0;         // 0=bloom | 1=distorção | 2=holograma
uniform float intensidade = 1.0;

void fragment() {
	vec2 uv = UV;
	vec2 uv2 = uv;
	float t = TIME;

	// -------------------------
	// DETETAR ZONA
	// -------------------------
	bool ativa = false;
	if (zona_correta == 0 && uv.x < 0.33) ativa = true;
	if (zona_correta == 1 && uv.x >= 0.33 && uv.x < 0.66) ativa = true;
	if (zona_correta == 2 && uv.x >= 0.66) ativa = true;

	// -------------------------
	// MODOS DE PISTA
	// -------------------------
	if (modo == 1) {
		// DISTORÇÃO
		if (ativa) {
			uv2.y += sin(uv.x * 20.0 + t * 6.0) * 0.01 * intensidade;
		} else {
			uv2 += vec2(
				sin(t * 2.0 + uv.y * 10.0),
				cos(t * 2.0 + uv.x * 10.0)
			) * 0.015;
		}
	}

	vec4 c = texture(tex, uv2);

	if (modo == 0) {
		// BLOOM + PULSO
		float pulse = sin(t * 2.5) * 0.5 + 0.5;
		if (ativa) {
			c.rgb *= 1.3 + pulse * 0.7;
			c.rgb += vec3(0.3, 0.6, 1.0) * pulse * intensidade;
		} else {
			c.rgb *= 0.08;
		}
	}
	else if (modo == 1) {
		// DISTORÇÃO
		if (!ativa) c.rgb *= 0.05;
	}
	else if (modo == 2) {
		// HOLOGRAMA
		float scan = sin(uv.y * 80.0 + t * 6.0) * 0.1;
		if (ativa) {
			c.rgb += vec3(0.0, 0.5, 1.0) * (0.6 + scan) * intensidade;
		} else {
			c.rgb *= 0.05;
		}
	}

	ALBEDO = clamp(c.rgb, 0.0, 1.0);
}
"

